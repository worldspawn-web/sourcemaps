"Refract"
{
	"%notooltexture" "1"
	"$refractamount" ".05"
	"$refracttint" "{250 218 13}"
	"$bluramount" "1"
	"$ignorez" "1"


	"$scale" "[1 1]"

	"$bumpmap" "trazox64/lava/lava_refract/lava_dudv"
	"$normalmap" "trazox64/lava/lava_refract/normal"
	"$bumpframe" "0"


	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$normalmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30.00
		}
		"TextureScroll"
		{
			"texturescrollvar" "$bumptransform"
			"texturescrollrate" .1
			"texturescrollangle" 45.00
		}
	}
}
